  "bugs": {
    "url": "https://github.com/jlindaue/barcodeApp/issues"
  },
  "homepage": "https://github.com/jlindaue/barcodeApp#readme",
  "devDependencies": {
    "concurrently": "^7.5.0",
    "json-server": "^0.17.0"
  }
}
